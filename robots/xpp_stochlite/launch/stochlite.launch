<launch>
 
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="stochlite_rviz_urdf_robot_description" command="$(find xacro)/xacro --inorder '$(find stochlite_description)/urdf/stochlite.urdf'"/>
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_stochlite" pkg="xpp_stochlite" type="urdf_visualizer_stochlite" output="screen"/>
     
</launch>
